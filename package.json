{
  "name": "Spark",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "deploy": "react-native bundle && fastlane alpha",
    "test": "jest --runInBand --forceExit --coverage",
    "test-watch": "jest --runInBand --forceExit --coverage --watch",
    "lint": "node_modules/eslint/bin/eslint.js 'src/**/*.js' '__tests__/**/*.js'",
    "precommit": "npm run lint",
    "prepush": "npm test",
    "postmerge": "npm install"
  },
  "dependencies": {
    "babel-preset-react-native-stage-0": "^1.0.1",
    "eslint": "^3.15.0",
    "hoist-non-react-statics": "^1.2.0",
    "immutability-helper": "^2.1.1",
    "lodash": "^4.17.4",
    "moment": "^2.17.1",
    "prop-types": "^15.5.10",
    "react": "^16.0.0-alpha.6",
    "react-native": "^0.44.3",
    "react-native-branch": "^2.0.0-beta.6",
    "react-native-config": "^0.3.1",
    "react-native-datepicker": "^1.4.4",
    "react-native-dropdownalert": "^2.8.0",
    "react-native-fabric": "^0.4.1",
    "react-native-fcm": "^7.1.0",
    "react-native-google-places-autocomplete": "^1.2.7",
    "react-native-image-picker": "^0.25.1",
    "react-native-keyboard-aware-scroll-view": "^0.2.7",
    "react-native-modal": "^2.4.0",
    "react-native-push-notification": "^3.0.0",
    "react-native-vector-icons": "^4.0.0",
    "react-navigation": "^1.0.0-beta.10",
    "react-redux": "^5.0.4",
    "redux": "^3.6.0",
    "redux-action-buffer": "^1.0.1",
    "redux-form": "^6.4.3",
    "redux-persist": "^4.4.0",
    "redux-thunk": "^2.1.0",
    "socket.io-client": "^1.5.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-jest": "18.0.0",
    "babel-preset-react-native": "1.9.1",
    "eslint": "^3.13.1",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "husky": "^0.13.1",
    "jest": "18.1.0",
    "react-test-renderer": "15.4.2",
    "remote-redux-devtools": "^0.5.12"
  },
  "jest": {
    "preset": "react-native",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-navigation|react-native-branch)"
    ],
    "modulePathIgnorePatterns": [
      "async"
    ],
    "setupFiles": [
      "./jest/setup.js"
    ]
  }
}
